        <!-- Carousel Start -->
        <div class="carousel-header">
            <div id="carouselId" class="carousel slide" data-bs-ride="carousel">
                <ol class="carousel-indicators">
                    <% for (let i=0; i < 3; i++) { %>
                        <li data-bs-target="#carouselId" data-bs-slide-to="<%= i %>" <% if (i===0) { %>class="active"<%
                                } %>></li>
                        <% } %>
                </ol>
                <div class="carousel-inner" role="listbox">
                    <% const images=["public/img/img2.jpg", "public/img/img3.jpg" , "public/img/img4.jpeg" ]; %>
                        <% const captions=[ "Let's Explore Pakistan Together!"
                            , "Find Your Perfect Tour At Pak Railways" , "You Like To Go?" ]; %>
                            <% for (let i=0; i < images.length; i++) { %>
                                <div class="carousel-item <% if (i === 0) { %>active<% } %>">
                                    <img src="<%= images[i] %>" class="img-fluid" alt="Image">
                                    <div class="carousel-caption">
                                        <div class="p-3" style="max-width: 900px;">
                                            <h1 class="display-2 text-capitalize text-white mb-4">
                                                <%= captions[i] %>
                                            </h1>
                                            <div class="d-flex align-items-center justify-content-center">
                                                <a class="btn  btn-outline-dark-0 rounded-pill py-3 px-5"
                                                    href="#">Discover Now</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <% } %>
