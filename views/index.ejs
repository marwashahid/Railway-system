                <div class="tab-content">
                    <div id="NationalTab-1" class="tab-pane fade show p-0 active">
                        <div class="row g-3">
                            <div class="col">
                                <label for="arrivalTime">Arrival</label>
                                <select id="arrivalTime" class="form-control" aria-label="Arrival Time">
                                    <option value="">-- Select Arrival --</option>
                                    <option value="Lahore">Lahore</option>
                                    <option value="Karachi">Karachi</option>
                                    <option value="Islamabad">Islamabad</option>
                                </select>
                            </div>
                            <div class="col">
                                <label for="deptTime">Departure</label>
                                <select id="deptTime" class="form-control" aria-label="Arrival Time">
                                    <option value="">-- Select Departure --</option>
                                    <option value="Faisalabad">Faisalabad</option>
                                    <option value="Lahore">Lahore</option>
                                    <option value="Karachi Cantt">Karachi Cantt</option>
                                </select>
                            </div>
                            <div class="col">
                                <label>Date time</label>
                                <input type="date" class="form-control" placeholder="Date" aria-label="Last name">
                            </div>
                            <div class="col-12">
                                <a id="searchTrain1"
                                    class="btn btn-outline-dark-0 rounded-pill py-2 px-4 mt-1">Search</a>
                            </div>
                        </div>
                    </div>
                    <div id="InternationalTab-2" class="tab-pane fade show p-0">
                        <div class="row g-3">
                            <div class="col">
                                <label for="train">Train</label>
                                <select id="train" class="form-control" aria-label="Arrival Time">
                                    <option value="">-- Select Train --</option>
                                    <option value="08:00">08:00 AM</option>
                                    <option value="09:00">09:00 AM</option>
                                    <option value="10:00">10:00 AM</option>
                                </select>
                            </div>
                            <div class="col-12">
                                <a id="searchTrain2" class="btn btn-outline-dark-0 rounded-pill py-2 px-4 mt-1"
                                    href="">Search</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <div class="mapDiv">
        <div id="map">

        </div>
    </div>
