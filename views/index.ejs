    <script>

        document.getElementById("searchTrain1").addEventListener("click", function () {
            let input1Value = document.getElementById("arrivalTime").value;
            let input2Value = document.getElementById("deptTime").value;

            fetch(`/search?input1=${input1Value}&input2=${input2Value}`, { method: "GET" })
                .then(response => response.json())
                .then(data => {
                    // Handle response data and render table
                    let tableHtml = "<table><thead><tr><th>Column 1</th><th>Column 2</th></tr></thead><tbody>";
                    data.forEach(row => {
                        tableHtml += `<tr><td>${row.column1}</td><td>${row.column2}</td></tr>`;
                    });
                    tableHtml += "</tbody></table>";
                    document.getElementById("resultTable").innerHTML = tableHtml;
                })
                .catch(error => {
                    console.error('Error:', error);
                });
        });
    </script>
